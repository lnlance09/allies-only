{
	"presets": [
		[
			"next/babel",
			{
				"preset-env": {
					"useBuiltIns": "usage",
					"corejs": "3.6"
				}
			}
		],
	],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"@actions": "./redux/actions",
					"@components": "./components",
					"@controllers": "./controllers",
					"@dummy": "./dummy",
					"@interfaces": "./interfaces",
					"@layouts": "./layouts",
					"@lib": "./lib",
					"@models": "./models",
					"@options": "./options",
					"@public": "./public",
					"@reducers": "./redux/reducers",
					"@redux": "./redux",
					"@static": "./static",
					"@style": "./style",
					"@store": "./store",
					"@sagas": "./sagas",
					"@utils": "./utils"
				},
				"extensions": [".ts", ".tsx"]
			}
		]
	],
	"env": {
		"test": {
			"presets": [["@babel/preset-env", { "modules": false } ], "next/babel"]
		}
	}
}